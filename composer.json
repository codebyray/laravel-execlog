{
  "name": "codebyray/laravel-execlog",
  "description": "Executive log for users",
  "license": "MIT",
  "authors": [
    {
      "name": "Ray Cuzzart II",
      "email": "dev@codebyray.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php" : "^7.1",
    "illuminate/config": "~5.5.0|~5.6.0|~5.7.0",
    "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3|^7.0",
    "orchestra/testbench": "~3.5.0|~3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Codebyray\\LaravelExeclog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codebyray\\LaravelExeclog\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Codebyray\\LaravelExeclog\\LaravelExeclogServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:windows": "vendor\\bin\\phpunit"
  }
}